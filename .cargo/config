[alias]
pull-test-suite = "run --package tools --bin tools -q -- pull-test-suite"
pull-locales    = "run --package tools --bin tools -q -- pull-locales"
pull-styles     = "run --package tools --bin tools -q -- pull-styles"
test-suite      = "run --package tools --bin tools -q -- test-suite"
inspect         = "-Z package-features run --package tools --features inspect --bin inspect -q -- "
tools           = "run --package tools --bin tools -q --"
