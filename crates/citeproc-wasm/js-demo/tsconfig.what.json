{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "React.createElement"
    },
    "include": [
        "js/**/*"
    ],
    "exclude": ["node_modules"]
}